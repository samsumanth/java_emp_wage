
package javapackage;

public class Uc1 {
	public static void main(String[] args) {
		// constants
		int IS_FULL_TIME = 1;
		
			//computation
		double empCheck = Math.floor(Math.random() * 10) % 2;
		
		if (empCheck ==IS_FULL_TIME)
			System.out.println("EMPLOYEE IS PRESENT");
		else
			System.out.println("EMPLOYEE IS ABSENT");
				
	}
}
